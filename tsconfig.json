{
	// Files
	// "files": [],
	// "include": [],
	// "exclude": [],
	// "references": [],
	
	// Compiler
	"compilerOptions": {

		// Type Checking
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"alwaysStrict": true,
		"exactOptionalPropertyTypes": true,
		"noFallthroughCasesInSwitch": false,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noPropertyAccessFromIndexSignature": true,
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"strict": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"useUnknownInCatchVariables": true,

		// Modules
		"allowArbitraryExtensions": true,
		"allowUmdGlobalAccess": false,
		"baseUrl": "./",
		"customConditions": [],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"moduleSuffixes": [""],
		"noResolve": false,
		"paths": {},
		"resolveJsonModule": true,
		"resolvePackageJsonExports": true,
		"resolvePackageJsonImports": true,
		"rootDir": "./ProTeamManager",
		"typeRoots": [],
		"types": [],

		// Emit
		"declaration": true,
		"declarationDir": "./build",
		"declarationMap": true,
		"downlevelIteration": true,
		"emitBOM": true,
		"emitDeclarationOnly": false,
		"importHelpers": true,
		"importsNotUsedAsValues": "remove",
		"inlineSourceMap": false,
		"inlineSources": true,
		"mapRoot": "./build",
		"newLine": "lf",
		"noEmit": false,
		"noEmitHelpers": false,
		"noEmitOnError": true,
		"outDir": "./build",
		"preserveConstEnums": true,
		"preserveValueImports": false,
		"removeComments": false,
		"sourceMap": true,
		"sourceRoot": "./build",
		"stripInternal": true,

		// JS
		"allowJs": true,
		"checkJs": true,
		"maxNodeModuleJsDepth": 0,
		
		// Editor
		"disableSizeLimit": true,
		"plugins": [{
        	"name": "ts-sql-plugin",
        	"command": "psql -c", // optionnal
        	"tags": { // optionnal
          		"sql": "sql",
          		"raw": "raw",
          		"cond": "cond",
          		"and": "and",
          		"or": "or",
          		"ins": "ins",
          		"upd": "upd",
          		"mock": "mock"
        	},
        	"mock": "0",
        	"cost_pattern": "/\\(cost=\\d+\\.?\\d*\\.\\.(\\d+\\.?\\d*)/",
        	"error_cost": null, // 100,
        	"warn_cost": null, // 50,
        	"info_cost": null, // -1,
        	"schema_command": "pg"
      	}],

		// Interop
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": false,
		"preserveSymlinks": false,

		//Compability
		// "charset": "utf8",
		"keyofStringsOnly": false,
		"noStrictGenericChecks": false,
		// "out": "./build"
		"suppressExcessPropertyErrors": false,
		"suppressImplicitAnyIndexErrors": false,

		// Environment
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		// "jsx": preserve
		// "jsxFactory": "React.createElement"
		// "jsxFragmentFactory": "React.Fragment"
		// "jsxImportSource": "react"
		"lib": ["ESnext"],
		"moduleDetection": "auto",
		"noLib": false,
		// "reactNamespace": "React"
		"target": "ESNext",
		"useDefineForClassFields": true,

		// Diagnostics
		// "diagnostics": true
		// "explainFiles": false
		"extendedDiagnostics": false,
		"generateCpuProfile": "profile.cpuprofile",
		"listEmittedFiles": false,
		"listFiles": false,
		"traceResolution": false,

		// Projects
		"composite": true,
		"disableReferencedProjectLoad": false,
		"disableSolutionSearching": false,
		"disableSourceOfProjectReferenceRedirect": false,
		"incremental": true,
		"tsBuildInfoFile": ".tsbuildinfo",

		// Output formatting
		"noErrorTruncation": true,
		"preserveWatchOutput": false,
		"pretty": true,

		// Completeness
		"skipDefaultLibCheck": false,

		// CLI
		
		// Watch
	},
	"watchOptions": {
		// "watchFile": "ProTeamManager/server.ts"
		"watchDirectory": "dynamicPriorityPolling",
		"fallbackPolling": "dynamicpriority",
		"synchronousWatchDirectory": false,
		"excludeDirectories": ["./node_modules", "build", "release"],
		"excludeFiles": [".env", "package-lock.json", "yarn.lock", "package.json", "License"]
	},

	"typeAquisition": {
		"enable": false
	}
}
